{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "new-cap": "off",
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreComments": true,
        "ignoreTrailingComments": true
      }
    ],
    "object-curly-spacing": "off",
    "require-jsdoc": "off"
  }
}